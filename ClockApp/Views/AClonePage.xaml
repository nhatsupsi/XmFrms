<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ClockApp.Core.Forms.Views.AClonePage">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="add" x:Name='addButton' HorizontalOptions="Center" VerticalOptions="Center" Clicked="OnButtonClicked"></Button>
            <Button Text="remove" x:Name='removeButton' HorizontalOptions="Center" VerticalOptions="Center" IsEnabled="False" Clicked="OnButtonClicked"></Button>

            <Label Text="Clock" x:Name='clockLabel' HorizontalOptions="Center" VerticalOptions="Center"></Label>
            <StackLayout x:Name='loggerLayout'></StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>